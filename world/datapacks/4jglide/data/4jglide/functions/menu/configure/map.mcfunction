##Open GUI if set to 1000
execute as @s[scores={4j.gamecfg=1000}] run function 4jglide:menu/load/host/mapgui

##Disable Canyon if set to 1001
execute as @s[scores={4j.gamecfg=1001}] run function 4jglide:menu/load/host/map/canyon/disable

##Enable Canyon if set to 1002
execute as @s[scores={4j.gamecfg=1002}] run function 4jglide:menu/load/host/map/canyon/enable

##Disable Temple if set to 1003
execute as @s[scores={4j.gamecfg=1003}] run function 4jglide:menu/load/host/map/temple/disable

##Enable Temple if set to 1004
execute as @s[scores={4j.gamecfg=1004}] run function 4jglide:menu/load/host/map/temple/enable

##Disable Cavern if set to 1005
execute as @s[scores={4j.gamecfg=1005}] run function 4jglide:menu/load/host/map/cavern/disable

##Enable Cavern if set to 1006
execute as @s[scores={4j.gamecfg=1006}] run function 4jglide:menu/load/host/map/cavern/enable

